RETARDO:    
    MOVLW   .100    ;SE ASIGNA UN VALOR ALEATORIO AL CONTADOR 1
    MOVWF   C1
    MOVLW   .104    ;SE ASIGNA EL VALOR DE LA ECUACION AL CONTADOR 2
    MOVWF   CONT2
    DECFSZ  CONT2   ;DECREMENTA EL CONTADOR 2 Y SALTA A DECREMENTAR EL CONT1 SI CONT2 = 0
    GOTO    $-1     ;REGRESA A DECREMENTAR EL CONT2
    DECFSZ  C1      ;DECREMENTA EL CONT1 Y SALTA AL RETURN SI CONT1 = 0
    GOTO    $-5     ;REGRESA A ASIGNAR VALOR AL CONT2
    
    RETURN

RETARDO2:
    MOVLW   .100
    MOVWF   CONTX
    MOVLW   .54
    MOVWF   CONTY
    DECFSZ  CONTY
    GOTO    $-1
    DECFSZ  CONTX
    GOTO    $-5
    
    RETURN


